<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Click the "Enable" element at the dropdown menu.</description>
  <action name="openDropDownMenu"/>
  <command name="click" target="id=action-dropdown"/>
  <command name="waitForVisible" target="css=.dropdown-menu"/>
  <action name="clickIntoDropDownMenu"/>
  <command name="click" target="css=.enable-selected">
    <comment>.disable-selected</comment>
  </command>
  <command name="waitForNotVisible" target="css=.dropdown-menu"/>
</scriptmodule>