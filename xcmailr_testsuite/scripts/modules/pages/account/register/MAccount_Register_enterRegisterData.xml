<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>enter first and last name, email, password, language</description>
  <parameter name="firstName"/>
  <parameter name="lastName"/>
  <parameter name="email"/>
  <parameter name="password"/>
  <parameter name="language"/>
  <codecomment>first name</codecomment>
  <command name="type" target="css=#inputRegisterFirstName" value="@{firstName}"/>
  <codecomment>last name</codecomment>
  <command name="type" target="css=#inputRegisterSurName" value="@{lastName}"/>
  <codecomment>E-Mail</codecomment>
  <command name="type" target="css=#inputRegisterMail" value="@{email}"/>
  <codecomment>Password</codecomment>
  <command name="type" target="css=#inputRegisterPassword" value="@{password}"/>
  <command name="type" target="css=#inputRegisterPasswordRepetition" value="@{password}"/>
  <codecomment>language</codecomment>
  <command name="select" target="css=#selectRegisterLanguage" value="value=@{language}"/>
</scriptmodule>