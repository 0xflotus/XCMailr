<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <parameter name="price"/>
  <parameter name="curSym"/>
  <parameter name="priceDigitGroupSeparators"/>
  <parameter name="price_varName"/>
  <command name="storeEval" target="&quot;@{curSym} &quot; + &quot;@{price}&quot;.replace(/\B(?=(\d{3})+(?!\d))/g, &quot;@{priceDigitGroupSeparators}&quot;)" value="@{price_varName}">
    <comment>Splits any length digit string into groups of three elements beginning from end of line separated by given delimiter. </comment>
  </command>
</scriptmodule>