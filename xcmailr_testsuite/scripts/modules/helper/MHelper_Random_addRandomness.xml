<?xml version="1.0" encoding="UTF-8"?>
<scriptmodule xmlns="http://xlt.xceptance.com/xlt-script/2" version="6">
  <description>Create a random string.</description>
  <parameter name="inputString"/>
  <parameter name="returnValue_varDynamic"/>
  <command name="store" target="${RANDOM.Number(1,5)}" value="rLength">
    <comment>Just a random value for future use.</comment>
  </command>
  <command name="store" target="@{inputString}${RANDOM.String(${rLength})}" value="@{returnValue_varDynamic}">
    <comment>Add some random characters to the input string.</comment>
  </command>
</scriptmodule>