<?xml version="1.0" encoding="UTF-8" ?>
<scriptmodule version="3" xmlns="http://xlt.xceptance.com/xlt-script/2">
  <description>Selects a random expiration date of a forwarding email by using the datepicker.</description>
  <command name="click" target="css=.add-on"/>
  <command name="click" target="css=.next"/>
  <command name="click" target="xpath=html/body/div[2]/ul/li[1]/div/div[1]/table/tbody/tr/td[@class='day' and contains(., '${RANDOM.Number(1,28)}')]"/>
  <command name="click" target="css=.icon-time"/>
  <command name="click" target="xpath=html/body/div[2]/ul/li[3]/div/div[1]/table/tbody/tr[1]/td[1]/a"/>
  <command name="click" target="xpath=html/body/div[2]/ul/li[3]/div/div[1]/table/tbody/tr[1]/td[3]/a"/>
  <command name="click" target="xpath=html/body/div[2]/ul/li[3]/div/div[1]/table/tbody/tr[3]/td[5]/a"/>
  <command name="click" target="xpath=html/body/div[2]/ul/li[2]"/>
</scriptmodule>
